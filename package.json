{
  "name": "@shawbs/ms-ui",
  "version": "0.1.21",
  "author": "shawbs",
  "description": "A mobile Vue component library",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "build-component": "webpack --config webpack.component.js",
    "build-theme": "gulp clean && gulp build && gulp copy",
    "i18n": "gulp i18n",
    "deploy": "webpack && npm run build-component && npm run build-theme && npm run i18n"
  },
  "keywords": [
    "ms",
    "vue",
    "components"
  ],
  "license": "MIT",
  "files": [
    "lib",
    "src/main"
  ],
  "main": "lib/dist/ms-ui.common.js",
  "style": "lib/theme/index.css",
  "unpkg": "lib/dist/ms-ui.umd.js",
  "dependencies": {
    "async-validator": "^3.2.0",
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.12.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
